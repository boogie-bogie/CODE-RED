<div class='container'>
  <img
    src='https://img1.daumcdn.net/thumb/R1280x0/?scode=mtistory2&fname=https%3A%2F%2Fblog.kakaocdn.net%2Fdn%2FoFvs6%2FbtsGHblHYxt%2F1kXhsa7cOG2FcjC2UzUIF1%2Fimg.png'
    alt='프로필 사진'
    id='profileImage'
  />
  <h1>CODE:RED</h1>

  <form action='/auth/sign-up' method='post' enctype='multipart/form-data'>

    <label for='email'>이메일</label>
    <input type='email' id='email' name='email' required />

    <label for='password'>비밀번호</label>
    <input type='password' id='password' name='password' required />

    <label for='passwordConfirm'>비밀번호 확인</label>
    <input
      type='password'
      id='passwordConfirm'
      name='passwordConfirm'
      required
    />

    <label for='name'>이름</label>
    <input type='text' id='name' name='name' required />

    <label for='nickname'>별명</label>
    <input type='text' id='nickname' name='nickname' required />

    <label for='phone_number'>전화번호</label>
    <input type='tel' id='phone_number' name='phone_number' required />

    <label for='profile_image'>프로필사진</label>
    <input type='file' id='profile_image' name='image' />

    <button class='submitButton' type='submit'>회원가입</button>
  </form>
</div>